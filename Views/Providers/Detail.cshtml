@using Search4Support.Models

@model Search4Support.ViewModels.ProviderDetailViewModel


<h1>@Model.Name</h1>

<table class="table">
 
    <tr>
        <th>Phone Number</th>
        <td>@Model.PhoneNumber</td>
    </tr>
    <tr>
        <th>Address</th>
        <td>@Model.Address</td>
    </tr>
    <tr>
        <th>Description</th>
        <td>@Model.Description</td>
    </tr>
    <tr>
        <th>Services</th>
        
      
            @foreach( var srv in Model.Services)
            { 
                <td>@srv.Name</td>
            }

        
    
    </tr>
</table>
  
