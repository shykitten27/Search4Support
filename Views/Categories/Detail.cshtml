@using Search4Support.Models
@model Search4Support.ViewModels.CategoryDetailViewModel


<h1>@Model.Name</h1>

<table class="table">

    <tr>
        <th>Services</th>
        <td>
            @Model.Services  
            
            @foreach (var srv in Model.Services)
            {

                <a asp-controller="Services" asp-action="Detail" asp-route-id="@Model.Services.">@srv.Name</a>
            }

        </td>

    </tr>
</table>